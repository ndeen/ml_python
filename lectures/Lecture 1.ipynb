   "source": [
    "x = [2,3,7,8,9]\n",
    "\n",
    "# loop\n",
    "y = []\n",
    "for e in x:\n",
    "    if e % 2 == 0:\n",
    "        y.append(e)\n",
    "        \n",
    "print(y)\n",
    "\n",
    "# comprehension\n",
    "z = [e for e in x if e % 2 == 0]\n",
    "\n",
    "print(z)"
   ]
